#
# Borland C makefile for UN_ARC.C ( COMPACT model )
#
.c.obj:
     bcc -mc -c -P $*.c

un_arc.exe: un_arc.obj sayerror.obj rw_io.obj dir_io.obj lzw4c_c.lib
     bcc -mc -M un_arc.obj sayerror.obj rw_io.obj dir_io.obj lzw4c_c.lib

un_arc.obj: un_arc.c

sayerror.obj: sayerror.c

rw_io.obj: rw_io.c

dir_io.obj: dir_io.c
