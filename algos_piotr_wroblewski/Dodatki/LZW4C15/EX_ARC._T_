#
# Turbo C makefile for EX_ARC.C ( COMPACT model )
#
.c.obj:
     tcc -mc -c -P $*.c

ex_arc.exe: ex_arc.obj sayerror.obj rw_io.obj dir_io.obj lzw4c_c.lib
     tcc -mc -M ex_arc.obj sayerror.obj rw_io.obj dir_io.obj lzw4c_c.lib

ex_arc.obj: ex_arc.c

sayerror.obj: sayerror.c

rw_io.obj: rw_io.c

dir_io.obj: dir_io.c
